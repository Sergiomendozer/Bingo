B_list_drawn_list = []
B_list_drawn_str = "1 5 3"
B_list_drawn_str = B_list_drawn_str.split()
for e in B_list_drawn_str:
    e = int(e)
    B_list_drawn_list.append(e)
    B_list_drawn_list.sort()
    B_list_drawn_str = " ".join([str(element) for element in B_list_drawn_list])


print(B_list_drawn_str)
print(B_list_drawn_list)

# = ' '.join([str(element) for element in lines])
